<h1><%= title %></h1>

<form action="/users/create" method="post">
  <label>
    Username:
    <input name="username" />
  </label>
  <input type="submit" value="Create user" />
</form>

<ul>
  <% users.forEach(function(user) { %>
    <li>
      <div>
        <%= user.username %>
      </div>
      <div>
        <form action="/users/<%= user.id %>/delete" method="post">
          <input type="submit" value="delete" />
        </form>
      </div>
    </li>
  <% }) %>
</ul>
